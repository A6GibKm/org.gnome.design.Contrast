{
    "id": "org.gnome.design.Contrast",
    "runtime": "org.gnome.Platform",
    "runtime-version": "44",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "contrast",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--metadata=X-DConf=migrate-path=/org/gnome/design/Contrast/"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin"
    },
    "modules": [
        {
            "name": "contrast",
            "buildsystem": "meson",
            "sources": [{
                "type": "archive",
                "url": "https://gitlab.gnome.org/World/design/contrast/uploads/05bb39467d74bfb535c41d64eb03c12f/contrast-0.0.7.tar.xz",
                "sha256": "eb1e4bfb60fc5072005136f77455df9a272c581551b9781558cfc29bfd0b72af"
            }]
        }
    ]
}
