{
    "id": "org.gnome.design.Contrast",
    "runtime": "org.gnome.Platform",
    "runtime-version": "44",
    "sdk": "org.gnome.Sdk",
    "sdk-extensions": [
        "org.freedesktop.Sdk.Extension.rust-stable"
    ],
    "command": "contrast",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--device=dri",
        "--metadata=X-DConf=migrate-path=/org/gnome/design/Contrast/"
    ],
    "build-options": {
        "append-path": "/usr/lib/sdk/rust-stable/bin"
    },
    "modules": [
        {
            "name": "contrast",
            "buildsystem": "meson",
            "sources": [{
                "type": "archive",
                "url": "https://gitlab.gnome.org/World/design/contrast/uploads/8bd569030ee2cb83537565471ea975f5/contrast-0.0.8.tar.xz",
                "sha256": "69c0d5764783048b7de79c9c6c69367fa38ac9bd427e9b5163cb00fbdfcdac97"
            }]
        }
    ]
}
